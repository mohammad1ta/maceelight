{
  "plugins": [ "node" ],
  "extends": [ "eslint:recommended", "plugin:node/recommended" ],
  "env": {},
  "globals": {},
  "rules": {
    "node/no-unsupported-features": [
      "error",
      {
				"version": 6,
				"ignores": [
					"Symbol.hasInstance"
				]
      }
    ],
    "no-irregular-whitespace": 2,
    "quotes": [
      2,
      "single"
    ]
  }
}
